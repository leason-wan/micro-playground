{
  "name": "Cookie Clearer",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": ["cookies"],
  "icons": {
    "16": "icons/16-beta.png",
    "48": "icons/48-beta.png",
    "128": "icons/128-beta.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    "next.html",
    "devtools.html",
    "devtools-background.html"
  ],
  "devtools_page": "devtools-background.html"
}